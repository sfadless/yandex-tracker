{
  "name": "sfadless/yandex-tracker",
  "license": "proprietary",
  "description": "Библиотека для взаимодействия с Яндекс Трекером",
  "version": "0.0.2-dev",
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Sfadless\\YandexTracker\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sfadless\\YandexTracker\\Test\\" : "tests/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-curl": "*",
    "symfony/http-client" : "^4.4 || ^5.4 || ^6.1",
    "symfony/options-resolver": "^4.4 || ^5.4 || ^6.1"
  },
  "require-dev": {
    "symfony/var-dumper": "6.3.x-dev",
    "phpstan/phpstan": "^1.8",
    "phpunit/phpunit": "^9.5"
  }
}
